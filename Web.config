<configuration>
   <system.webServer>
      <rewrite>
         <rules>
            <rule name="ProductList">
               <match url="(^[^\\.]*$ /index.html [L])" />
               <action type="Redirect" url="/" />
            </rule>
         </rules>
      </rewrite>
      <staticContent>
         <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
         <mimeMap fileExtension=".webm" mimeType="video/webm" />
      </staticContent>
   </system.webServer>
</configuration>